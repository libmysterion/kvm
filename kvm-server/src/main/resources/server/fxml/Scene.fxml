<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns:fx="http://javafx.com/fxml" fx:controller="com.mystery.kvm.MonitorSetupController">
    <children>
        <VBox id="VBox" alignment="CENTER" spacing="5.0">
            <children>
                <HBox id="HBox" alignment="CENTER" spacing="5.0">
                    <children>
                        <TableView fx:id="monitorsTable" prefHeight="500.0" prefWidth="500.0"/>
                        <ListView fx:id="availableMonitorsListBox" prefHeight="500.0" prefWidth="200.0" />            
                    </children>
                </HBox>
                <HBox alignment="CENTER" spacing="5.0">
                    <children>
                        <Button fx:id="startButton" text="Start"/>
                    </children>
                </HBox>
            </children>
        </VBox>
        
    </children>
</AnchorPane>